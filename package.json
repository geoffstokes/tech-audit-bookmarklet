{
  "name": "tech-audit-bookmarklet",
  "version": "1.0.0",
  "description": "Bookmarklet for quickly detecting technologies in use on the web",
  "scripts": {
    "build": "babel tech-audit.js --out-file docs/tech-audit.bookmarklet.js",
    "lint": "./node_modules/.bin/eslint tech-audit.js",
    "lint-and-fixup": "./node_modules/.bin/eslint --fix tech-audit.js",
    "serve": "http-server docs/"
  },
  "repository": "https://github.com/ticky/tech-audit-bookmarklet.git",
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-babili": "^0.0.11",
    "babel-preset-env": "^1.1.11",
    "http-server": "^0.9.0"
  },
  "babel": {
    "presets": [
      [
        "babili",
        {
          "mangle": {
            "topLevel": true,
            "eval": true
          }
        }
      ],
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 5%",
              "last 2 versions"
            ],
            "modules": false
          }
        }
      ]
    ]
  }
}
